<%- include('header') %>

  <main class="wrapper">
    <% data.forEach((item)=> { %>
      <div>
        <h3>
          <%= item.name %>
        </h3>
        <p>Day of Week: <%= item.dayOfWeek %>
        </p>
        <p>Meal Type: <%= item.mealType %>
        </p>
        <img src="<%= item.imageUrl %>" alt="Meal Image">
      </div>
      <% }); %>
  </main>


  <%- include('footer') %>